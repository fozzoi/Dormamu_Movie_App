{
  "expo": {
    "name": "Dormamu",
    "slug": "Dormamu",
    "version": "2.5.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "myapp",
    "developmentClient": false,
    "jsEngine": "hermes",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "androidNavigationBar": {
      "barStyle": "dark-content",
      "backgroundColor:": "#000"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "jsEngine": "hermes",
      "package": "com.Dormamu"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "592c63ff-f6ab-4bb0-9b5c-cf1b89eb6259"
      }
    },
    "ios": {
      "bundleIdentifier": "com.Dormamu"
    }
  }
}
